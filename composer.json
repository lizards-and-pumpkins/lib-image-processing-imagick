{
    "name": "lizards-and-pumpkins/lib-image-processing-imagick",
    "description": "Lizards & Pumpkins Image Processing Library For ImageMagick",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vinai Kopp",
            "email": "vinai@lizardsandpumpkins.com"
        },
        {
            "name": "Tim Bezhashvyly",
            "email": "tim@lizardsandpumpkins.com"
        },
        {
            "name": "Fabian Blechsmidt",
            "email": "fabian@lizardsandpumpkins.com"
        }
    ],
    "require": {
        "php": "~5.6.0|~7.0",
        "ext-imagick": "*",
        "lizards-and-pumpkins/catalog": "^0.8.0-alpha"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*",
        "lizards-and-pumpkins/coding-standards": "dev-master"
    },
    "autoload" : {
        "psr-4": { "LizardsAndPumpkins\\Import\\ImageStorage\\ImageProcessing\\ImageMagick\\": "src/" }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.lizardsandpumpkins.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit -c tests/Unit/phpunit.xml.dist",
        "sniff": "vendor/bin/phpcs --runtime-set ignore_warnings_on_exit true --runtime-set ignore_errors_on_exit true -p --colors --encoding=utf-8 --standard=vendor/lizards-and-pumpkins/coding-standards/src/LizardsAndPumpkins/ src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
